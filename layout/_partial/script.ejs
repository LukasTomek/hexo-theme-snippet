<% if(theme.jsonContent.searchLocal && !is_post()){ %>
<%- js('js/search.js?rev=@@hash') %>
<% } %>
<% if(theme.tagcloud.tag3d){ %>
    <%- js('assets/tagcanvas.min.js?rev=2.9') %>
    <script>
        var tagOption = {
            textColour: '<%= theme.tagcloud.textColour %>', // font color
            outlineMethod: '<%= theme.tagcloud.outlineMethod %>', // Selected mode
            outlineColour: '<%= theme.tagcloud.outlineColour %>', // The color of the selected mode
            interval: <%= theme.tagcloud.interval %> || 30, // The time interval between animation frames, the larger the value, the greater the rotation
            textHeight: 13,
            outlineRadius: 3,
            freezeActive: <%= theme.tagcloud.freezeActive %> || '', // Whether the selected label continues to scroll
            frontSelect: <%= theme.tagcloud.frontSelect %> || '', // Do not select the label at the back of the tag cloud
            initial: [0.1, -0.1],
            depth: 0.5,
            decel: 0.95,
            maxSpeed: 0.03,
            reverse: <%= theme.tagcloud.reverse %> || '', // Whether to trigger in the reverse
            fadeIn: 500, // Enter animation time
            wheelZoom: <%= theme.tagcloud.wheelZoom %> || '' // Whether to enable the mouse wheel
        }
        TagCanvas.Start('tag-cloud-3d','',tagOption);
    </script>
<% } %>
<% if(theme.visit_counter.site || theme.visit_counter.page){ %>
    <script async src="//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script>
<% } %>
<%- js('js/app.js?rev=@@hash') %>
